mixin social(data)
    ul.social.flex&attributes(attributes)
        li.social__item
            a.social__link(href="#")
                != pugHelpers.Icon.call(locals, {iconName: 'instagram-icon', className: 'social__icon'})
        li.social__item
            a.social__link(href="#")
                != pugHelpers.Icon.call(locals, {iconName: 'facebook-icon', className: 'social__icon'})